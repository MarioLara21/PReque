<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Gestion_Cubiculos">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="161dp"
        android:text="Gestión de \n cubículos"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="325dp"
        android:layout_height="395dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="42dp"
        android:orientation="horizontal"
        android:scrollbars="horizontal|vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="46dp"
                android:layout_height="wrap_content"
                android:minHeight="16sp"
                android:text="ID"
                android:textAlignment="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="77dp"
                android:layout_height="wrap_content"
                android:text="Nombre"
                android:textAlignment="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="84dp"
                android:layout_height="wrap_content"
                android:text="Capacidad"
                android:textAlignment="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="68dp"
                android:layout_height="wrap_content"
                android:text="Estado"
                android:textAlignment="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="48dp"
                android:layout_height="wrap_content"
                android:text="SP"
                android:textAlignment="center"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </TableLayout>

    <Button
        android:id="@+id/button6"
        android:layout_width="195dp"
        android:layout_height="48dp"
        android:layout_marginTop="13dp"
        android:text="Agregar Cubículo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout" />

    <Button
        android:id="@+id/button9"
        android:layout_width="197dp"
        android:layout_height="49dp"
        android:layout_marginTop="8dp"
        android:text="Eliminar Cubículo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Modificar Cubículo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9" />

    <Button
        android:id="@+id/button11"
        android:layout_width="198dp"
        android:layout_height="38dp"
        android:layout_marginTop="5dp"
        android:text="Volver"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button10" />
</androidx.constraintlayout.widget.ConstraintLayout>